{
	"name" : "slothsoft/cron",
	"description" : "A Farah module that handles cronjobs.",
	"license" : "MIT",
	"authors" : [{
			"name" : "Daniel Schulz",
			"email" : "info.slothsoft@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : "^7.2",
		"krakjoe/pthreads-polyfill" : "^1.1",
		"slothsoft/core" : "^1.10",
		"slothsoft/farah" : "^1.5.2"
	},
	"require-dev" : {
		"phpunit/phpunit" : "8.5.*"
	},
	"suggest" : {
		"ext-pthreads" : "*"
	},
	"autoload" : {
		"psr-4" : {
			"Slothsoft\\Cron\\" : "src"
		},
		"files" : [
			"scripts/bootstrap.php"
		]
	},
	"keywords" : [
		"slothsoft",
		"farah-module",
		"docs"
	],
	"autoload-dev" : {
		"psr-4" : {
			"Slothsoft\\Cron\\Tests\\" : "tests"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-main" : "1.0.x-dev"
		}
	},
	"homepage" : "http://farah.slothsoft.net/modules/cron"
}